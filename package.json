  "devDependencies": {
    "eslint": "4.13.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.8.0",
    "fs-extra": "5.0.0",
    "joi": "13.0.2",
    "klaw": "2.1.1"
  }
