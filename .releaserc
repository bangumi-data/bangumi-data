{
    "branches": [
        "master"
    ],
    "plugins": [
        [
            "@semantic-release/commit-analyzer",
            {
                "preset": "angular",
                "releaseRules": [
                    {
                        "type": "feat",
                        "release": "patch"
                    },
                    {
                        "type": "update",
                        "release": "patch"
                    }
                ]
            }
        ],
        "@semantic-release/npm",
        [
            "@semantic-release/github",
            {
                "successComment": false,
                "failComment": false
            }
        ],
        [
            "@semantic-release/git",
            {
                "assets": [
                    "dist/**/*.json"
                ],
                "message": "chore(release): ${nextRelease.version} [skip ci]"
            }
        ]
    ]
}
